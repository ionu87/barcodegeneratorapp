{
  "name": "barcodegeneratorapp",
  "version": "1.0.0",
  "description": "Barcode Generator Desktop App",
  "main": "electron/main.js",
  "author": "Ionut",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:build": "npm run build && electron-builder --publish never"
  },
  "build": {
    "appId": "com.ionut.barcodegenerator",
    "productName": "Barcode Generator",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/main.js",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  },
  "dependencies": {
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.4.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "24.13.3",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.1"
  }
}
